{# This is the template file, do not Edit it! -#}
aimrt:
  log:
    core_lvl: INFO # Trace/Debug/Info/Warn/Error/Fatal/Off
    backends:
      - type: console
  module:
    pkgs:
{%- for pkg in deploy.pkgs %}
      - path: ./{{ pkg.name }}.dll
        disable_modules: {{ pkg.disable_modules }}
{%- endfor %}
    modules:
{%- for pkg in deploy.pkgs %}
{%- for module in pkg.deployed_modules %}
      - name: {{ module }}
        log_lvl: INFO
{%- endfor %}
{%- endfor %}

